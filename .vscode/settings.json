{
  "[vue]": {
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.tabSize": 2
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": ["javascript"],
  "spellright.language": ["en"],
  "spellright.documentTypes": ["markdown", "latex", "plaintext", "yaml"]
}
